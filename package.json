{
  "name": "3d-force-graph",
  "version": "1.52.0",
  "description": "UI component for a 3D force-directed graph using ThreeJS and d3-force-3d layout engine",
  "unpkg": "dist/3d-force-graph.min.js",
  "main": "dist/3d-force-graph.common.js",
  "module": "dist/3d-force-graph.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/3d-force-graph.git"
  },
  "homepage": "https://github.com/vasturiano/3d-force-graph",
  "keywords": [
    "3d",
    "force",
    "graph",
    "three",
    "ngraph",
    "webgl"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/3d-force-graph/issues"
  },
  "scripts": {
    "prod": "npm run build",
    "build": "npm run clear && rollup -c && npm run minify",
    "build-es": "npm run clear && rollup -c rollup.config.es.js && npm run minify",
    "build-module": "npm run clear && rollup -c rollup.config.module.js && npm run minify-module",
    "clear": "rimraf dist",
    "build-org": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/3d-force-graph.js -o dist/3d-force-graph.min.js -c -m --comments '/Version/'",
    "minify-module": "npm run minify-module-cjs && npm run minify-module-es",
    "minify-module-cjs": "terser dist/3d-force-graph.common.js -o dist/3d-force-graph.common.min.js -c -m --comments '/Version/'", 
    "minify-module-es": "terser dist/3d-force-graph.module.js -o dist/3d-force-graph.module.min.js -c -m --comments '/Version/'",
    "prepare": "npm run build && npm run minify && npm run build-module",
    "watch": "rollup -c -w"
  },
  "files": [
    "dist/**/*",
    "example/**/*"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "accessor-fn": "^1.2.2",
    "kapsule": "^1.10.1",
    "three": "^0.107.0",
    "three-dragcontrols": "^0.88.2",
    "three-forcegraph": "^1.25.0",
    "three-render-objects": "^1.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.18.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "terser": "^4.1.2"
  }
}
